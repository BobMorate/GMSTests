            <string>key_left = -keyboard_check(vk_left); //all of these will return 1 when pressed
key_right = keyboard_check(vk_right);
key_up = -keyboard_check(vk_up);
key_down = keyboard_check(vk_down);
//key_space = keyboard_check(vk_space);

hmove = key_left + key_right;
vmove = key_up + key_down;
hsp = hmove * movespeed;        
vsp = vmove * movespeed;

//determine where the player is looking

if keyboard_check(vk_up) == 1
    {
    image_angle = 90
    }
    
if keyboard_check(vk_down) == 1
    {
    image_angle = 270
    }
     
if keyboard_check(vk_left) == 1
    {
    image_angle = 180
    }
     
if keyboard_check(vk_right) == 1
    {
    image_angle = 0
    }
  
facing = image_angle
    
//horizontal collision
if (place_meeting(x+hsp,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
    x +=sign(hsp);
    }
    hsp = 0
}

//vertical collision
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
    y +=sign(vsp);
    }
    vsp = 0
}

x += hsp;
y += vsp;

key_space = keyboard_check(vk_space);




</string>
