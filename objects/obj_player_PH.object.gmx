            <string>key_space = keyboard_check(vk_space);

//attack motion
if key_space == 1 &amp;&amp; can_attack == 1
    {
    if facing = 0
        {
        instance_create(x+16,y,obj_sword);
        obj_sword.image_angle = 0;
        can_attack = 0;
        alarm[0] = 20; //add direction to the attack to where the player is looking!
        } 
    if facing = 90
        {
        instance_create(x,y-16,obj_sword);
        obj_sword.image_angle = 90;
        can_attack = 0;
        alarm[0] = 20; //add direction to the attack to where the player is looking!
        }
    if facing = 270
        {
        instance_create(x,y+16,obj_sword);
        obj_sword.image_angle = 270;
        can_attack = 0;
        alarm[0] = 20; //add direction to the attack to where the player is looking!
        }
    if facing = 180
        {
        instance_create(x-16,y,obj_sword);
        obj_sword.image_angle = 180;
        can_attack = 0;
        alarm[0] = 20; //add direction to the attack to where the player is looking!
        }
    }

