draw_sprite(spr_healthbar,0,obj_player.x,obj_player.y - 60); //draws the healtbar, which will persist even if the oxygen bar is depleted.
for (xx = 0; xx &lt; current_oxygen; xx +=1 )
{
draw_sprite(spr_oxygen,0,obj_player.x+xx,obj_player.y - 60); 
draw_sprite(spr_healthbar,0,obj_player.x,obj_player.y - 60); //draws again the healthbar, over the oxygen bar, so the graphic does not overlap it.
}
//draw the health bar only when it's not at its maximum
if current_health &lt; maximum_health
    {
    for (xx = 0; xx &lt; current_health; xx +=1 )
        {
        draw_sprite(spr_health,0,instance_.x+xx,obj_player.y - 80); 
        draw_sprite(spr_healthbar,0,obj_player.x,obj_player.y - 80)
        }
    }


    

