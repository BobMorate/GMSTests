        <arguments>
          <argument>
            <kind>1</kind>
            <string>key_left = -keyboard_check(vk_left); //all of these will return 1 when pressed
key_right = keyboard_check(vk_right);
key_up = -keyboard_check(vk_up);
key_down = keyboard_check(vk_down);
//key_space = keyboard_check(vk_space);

hmove = key_left + key_right;
vmove = key_up + key_down;
hsp = hmove * movespeed;        
vsp = vmove * movespeed;

//determine where the player is looking

if keyboard_check(vk_nokey) == 1
    {
    image_speed = 0
    }


if key_up == -1 //-1 because of the variable on top would return -1!
    {
    sprite_index = spr_player_up
    image_speed = 0.1
    facing = 90
    }

   
    
if key_down == 1
    {
    sprite_index = spr_player_down
    image_speed = 0.1
    facing = 270
    }

     
if key_left == -1 //as with "up"
    {
    sprite_index = spr_player_left
    image_speed = 0.1
    facing = 180
    }

          
if key_right == 1
    {
    sprite_index = spr_player_right
    image_speed = 0.1
    facing = 0
    }

  

    
//horizontal collision
if (place_meeting(x+hsp,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
    x +=sign(hsp);
    }
    hsp = 0
}

//vertical collision
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
    y +=sign(vsp);
    }
    vsp = 0
}

x += hsp;
y += vsp;





</string>
          </argument>
        </arguments>
      </action>
